Host internet
    Hostname IPADDRESS
    Port 10022
    User sakura-user
    IdentityFile ~/work/sakura_design_pattern/11_lgwan_connect/.ssh/sshkey
    ControlMaster auto
    ControlPath ~/.ssh/cp-%r@%h:%p
    ControlPersist 10m

Host gateway
    Hostname 192.168.4.10
    User sakura-user
    IdentityFile ~/work/sakura_design_pattern/11_lgwan_connect/.ssh/sshkey
    ProxyJump internet

Host lgwan
    Hostname 192.168.6.2
    User sakura-user
    IdentityFile ~/work/sakura_design_pattern/11_lgwan_connect/.ssh/sshkey
    ProxyJump gateway
